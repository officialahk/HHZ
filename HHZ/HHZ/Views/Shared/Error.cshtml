@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<!--************************************
                Home Banner Start
        *************************************-->
<div class="tg-pageinnerbanner tg-haslayout tg-parallaximg" data-appear-top-offset="600" data-parallax="scroll" data-image-src="~/images//banner/img-02.jpg">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 col-xs-12">
                <div class="tg-pageheadcontent">
                    <div class="tg-pagetitle">
                        <h1>our latest news</h1>
                    </div>
                    <ol class="tg-breadcrumb">
                        <li><a href="#">Home</a></li>
                        <li class="tg-active">404 Error</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
</div>
<!--************************************
        Home Banner End
*************************************-->
<!--************************************
        Main Start
*************************************-->
<main id="tg-main" class="tg-main tg-haslayout">
    <div class="container">
        <div class="row">
            <div id="tg-content" class="tg-content">
                <div class="tg-404error">
                    <div class="col-sm-offset-3 col-sm-6 col-xs-offset-0 col-xs-12">
                        <div class="tg-404ontent">
                            <figure class="tg-404img">
                                <img src="~/images//404-img.jpg" alt="image description">
                            </figure>
                            <h2>Oops!</h2>
                            <div class="tg-description"><p>I think you turned the wrong way. Let’s go back to <a href="index.html">Homepage</a></p></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>
<!--************************************
        Main End
*************************************-->
